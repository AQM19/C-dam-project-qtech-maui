<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:drawable="clr-namespace:_3.Presentation.Data"
             x:Class="_3.Presentation.View.LecturasTerrarioPageView"
             Title="LecturasTerrarioPageView">
    <ContentPage.Resources>
        <drawable:GraphicsDrawable x:Key="drawable"/>
    </ContentPage.Resources>
    <VerticalStackLayout VerticalOptions="Center" HorizontalOptions="Center" Padding="30" Spacing="30">

        <GraphicsView Drawable="{StaticResource drawable}" x:Name="graphView" HeightRequest="300" />

    </VerticalStackLayout>
</ContentPage>